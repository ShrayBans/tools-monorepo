    "@uiw/react-md-editor": "^4.0.7",
    "ai-elements": "^1.0.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.10",
    "heic2any": "^0.0.4",
    "idb": "^8.0.3",
