{
  "name": "@shray/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./electron-main.js",
  "scripts": {
    "dev": "vite",
    "dev:prod": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check:ci": "tsc --incremental --noEmit",
    "type-check": "tsgo --incremental --noEmit",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "desktop:dev": "concurrently \"vite\" \"sleep 3 && node_modules/.bin/electron .\"",
    "desktop:build": "vite build && todesktop build",
    "desktop:release": "todesktop release --latest --force"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.14",
