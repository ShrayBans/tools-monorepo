    "@shray/ui": "workspace:*",
    "@hello-pangea/dnd": "^18.0.1",
    "@icons-pack/react-simple-icons": "^13.7.0",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/react-query": "4.36.1",
